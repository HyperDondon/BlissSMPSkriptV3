						set {puff.cooldown3.%attacker%} to true
					if {Bliss.Puff.%attacker%} is 3:	
						set {PuffMin3.%attacker%} to 1
						set {PuffSec3.%attacker%} to 15
						set {puff.cooldown3.%attacker%} to true
					if {Bliss.Puff.%attacker%} < 3:	
